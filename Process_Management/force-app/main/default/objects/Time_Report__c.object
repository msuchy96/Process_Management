        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Due_Date_Exceeded__c</fullName>
        <externalId>false</externalId>
        <formula>Date__c &gt;  Job__r.Due_Date_In_Date__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Due Date Exceeded</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Job_Reporter_Date_Id__c</fullName>
        <externalId>false</externalId>
        <formula>Job__c + Reporter__c + TEXT(Date__c)</formula>
